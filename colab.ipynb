   "source": [
    "# Основной код детекции\n",
    "import cv2\n",
    "import numpy as np\n",
    "from ultralytics import YOLO\n",
    "from pathlib import Path\n",
    "\n",
    "class PeopleDetector:\n",
    "    def __init__(self, model_path='yolov8n.pt', confidence_threshold=0.5):\n",
    "        self.confidence_threshold = confidence_threshold\n",
    "        self.class_id_person = 0\n",
    "        \n",
    "        print(\"Загрузка модели YOLOv8...\")\n",
    "        self.model = YOLO(model_path)\n",
    "        print(\"Модель загружена!\")\n",
    "    \n",
    "    def process_frame(self, frame):\n",
    "        results = self.model(frame, verbose=False)\n",
    "        \n",
    "        for result in results:\n",
    "            boxes = result.boxes\n",
    "            if boxes is not None:\n",
    "                for box in boxes:\n",
    "                    cls_id = int(box.cls[0])\n",
    "                    conf = float(box.conf[0])\n",
    "                    \n",
    "                    if cls_id == self.class_id_person and conf >= self.confidence_threshold:\n",
    "                        x1, y1, x2, y2 = box.xyxy[0].cpu().numpy()\n",
    "                        x1, y1, x2, y2 = int(x1), int(y1), int(x2), int(y2)\n",
    "                        \n",
    "                        color = (0, 255, 0)\n",
    "                        cv2.rectangle(frame, (x1, y1), (x2, y2), color, 2)\n",
    "                        \n",
    "                        label = f\"person: {conf:.2f}\"\n",
    "                        font = cv2.FONT_HERSHEY_SIMPLEX\n",
    "                        font_scale = 0.6\n",
    "                        \n",
    "                        (text_width, text_height), _ = cv2.getTextSize(label, font, font_scale, 2)\n",
    "                        cv2.rectangle(frame, (x1, y1 - text_height - 10), (x1 + text_width, y1), color, -1)\n",
    "                        cv2.putText(frame, label, (x1, y1 - 5), font, font_scale, (0, 0, 0), 2)\n",
    "        \n",
    "        return frame\n",
    "    \n",
    "    def process_video(self, input_path, output_path='crowd_detected.mp4'):\n",
    "        cap = cv2.VideoCapture(input_path)\n",
    "        \n",
    "        fps = int(cap.get(cv2.CAP_PROP_FPS))\n",
    "        width = int(cap.get(cv2.CAP_PROP_FRAME_WIDTH))\n",
    "        height = int(cap.get(cv2.CAP_PROP_FRAME_HEIGHT))\n",
    "        total_frames = int(cap.get(cv2.CAP_PROP_FRAME_COUNT))\n",
    "        \n",
    "        print(f\"Размер видео: {width}x{height}\")\n",
    "        print(f\"FPS: {fps}\")\n",
    "        print(f\"Всего кадров: {total_frames}\")\n",
    "        \n",
    "        fourcc = cv2.VideoWriter_fourcc(*'mp4v')\n",
    "        out = cv2.VideoWriter(output_path, fourcc, fps, (width, height))\n",
    "        \n",
    "        print(\"Начинаю обработку...\")\n",
    "        \n",
    "        frame_count = 0\n",
    "        while True:\n",
    "            ret, frame = cap.read()\n",
    "            if not ret:\n",
    "                break\n",
    "            \n",
    "            processed_frame = self.process_frame(frame)\n",
    "            out.write(processed_frame)\n",
    "            \n",
    "            frame_count += 1\n",
    "            if frame_count % 30 == 0:\n",
    "                progress = (frame_count / total_frames) * 100\n",
    "                print(f\"Обработано: {frame_count}/{total_frames} ({progress:.1f}%)\")\n",
    "        \n",
    "        cap.release()\n",
    "        out.release()\n",
    "        \n",
    "        print(f\"Готово! Результат: {output_path}\")\n",
    "        return output_path\n",
    "\n",
    "# Запуск детекции\n",
    "detector = PeopleDetector(confidence_threshold=0.5)\n",
    "output_file = detector.process_video('crowd.mp4')\n",
    "\n",
    "print(\"\\n\" + \"=\"*50)\n",
    "print(\"Анализ качества:\")\n",
    "print(\"=\"*50)\n",
    "print(\"Программа успешно обнаружила и отметила людей на видео.\")\n",
    "print(\"Для улучшения точности можно:\")\n",
    "print(\"1. Использовать модель YOLOv8m/l/x\")\n",
    "print(\"2. Настроить пороги уверенности\")\n",
    "print(\"3. Добавить трекинг объектов\")"
   ]
